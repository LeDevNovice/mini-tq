{
  "name": "@mini-tq/core",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "typecheck": "tsc -p tsconfig.json --noEmit",
